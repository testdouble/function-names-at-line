{
  "name": "function-names-at-line",
  "version": "1.1.0",
  "description": "Name the functions found at a particular line number in some JavaScript source",
  "main": "index.js",
  "scripts": {
    "ci": "npm test && npm run style",
    "test": "teenytest **/*.test.js --helper test/helper.js",
    "test:debug": "node-debug ./node_modules/.bin/teenytest **/*.test.js --helper test/helper.js",
    "style": "standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/testdouble/function-names-at-line.git"
  },
  "keywords": [
    "line",
    "numbers",
    "function",
    "names"
  ],
  "author": "Justin Searls <searls@gmail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/testdouble/function-names-at-line/issues"
  },
  "homepage": "https://github.com/testdouble/function-names-at-line#readme",
  "dependencies": {
    "acorn": "^7.1.1",
    "acorn-walk": "^7.1.1"
  },
  "devDependencies": {
    "lodash": "^4.17.15",
    "standard": "^14.3.3",
    "teenytest": "^3.1.0"
  },
  "standard": {
    "ignore": "test/fixtures/**/*"
  }
}
